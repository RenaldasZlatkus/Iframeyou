
<% if @settings.style == "2H" %>
	<iframe src="https://www.duckduckgo.com" class="full <%= @setting.background %>-bg <%= @setting.skin %>"></iframe>
	<iframe src="http://nycda.com/" class="full <%= @setting.background %>-bg <%= @setting.skin %>"></iframe>
<% elsif @settings.style == "2V" %>
	<iframe src="https://www.duckduckgo.com" id="leftfull <%= @setting.background %>-bg <%= @setting.skin %>"></iframe>
	<iframe src="http://nycda.com/" id="rightfull <%= @setting.background %>-bg <%= @setting.skin %>"></iframe>
<% end %>











